{
  "name": "chat-with-pdf",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix vite:build",
    "deploy": "bun run build && wrangler pages deploy",
    "dev": "remix vite:dev",
    "lint": "bunx @biomejs/biome lint --write .",
    "format": "bunx @biomejs/biome format --write .",
    "start": "wrangler pages dev ./build/client",
    "db:generate": "drizzle-kit generate --config=./app/lib/db/config.ts",
    "db:migrate": "drizzle-kit migrate --config=./app/lib/db/config.ts",
    "db:studio": "drizzle-kit studio --config=./app/lib/db/config.ts",
    "typecheck": "tsc",
    "typegen": "wrangler types",
    "preview": "bun run build && wrangler pages dev",
    "cf-typegen": "wrangler types",
    "vector-api:generate": "bunx openapi-typescript https://neon.tech/api_spec/release/v2.json -o ./app/lib/vector/api-schema.ts",
    "vector:generate": "drizzle-kit generate --config=./app/lib/vector/config.ts"
  },
  "dependencies": {
    "@ai-sdk/react": "^0.0.39",
    "@aws-sdk/client-s3": "^3.623.0",
    "@aws-sdk/s3-request-presigner": "^3.623.0",
    "@langchain/community": "^0.2.24",
    "@langchain/core": "^0.2.21",
    "@langchain/openai": "^0.2.6",
    "@neondatabase/serverless": "^0.9.4",
    "@radix-ui/react-avatar": "^1.1.0",
    "@remix-run/cloudflare": "^2.10.3",
    "@remix-run/cloudflare-pages": "^2.10.3",
    "@remix-run/react": "^2.10.3",
    "@tanstack/react-query": "^5.51.21",
    "ai": "^3.3.0",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.32.1",
    "drizzle-zod": "^0.5.1",
    "isbot": "^4.1.0",
    "langchain": "^0.2.13",
    "nanoid": "^5.0.7",
    "openapi-fetch": "^0.10.5",
    "react": "^18.2.0",
    "react-aria-components": "^1.3.1",
    "react-dom": "^18.2.0",
    "react-pdf": "^9.1.0",
    "remix-auth": "^3.7.0",
    "remix-auth-google": "^2.0.0",
    "remix-flat-routes": "^0.6.5",
    "tailwind-merge": "^2.4.0",
    "unpdf": "^0.11.0",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@cloudflare/workers-types": "^4.20240729.0",
    "@remix-run/dev": "^2.10.3",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "autoprefixer": "^10.4.19",
    "drizzle-kit": "^0.23.1",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.4",
    "tailwindcss-animate": "^1.0.7",
    "tailwindcss-react-aria-components": "^1.1.4",
    "typescript": "^5.1.6",
    "vite": "^5.1.0",
    "vite-tsconfig-paths": "^4.2.1",
    "wrangler": "3.57.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
